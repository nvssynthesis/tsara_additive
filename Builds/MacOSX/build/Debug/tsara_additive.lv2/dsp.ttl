@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix bufs:  <http://lv2plug.in/ns/ext/buf-size#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix opts:  <http://lv2plug.in/ns/ext/options#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix pg:    <http://lv2plug.in/ns/ext/port-groups#> .
@prefix plug:  <https://nvssynthesis.github.io//plugins/tsara_additive:> .
@prefix pprop: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rsz:   <http://lv2plug.in/ns/ext/resize-port#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix time:  <http://lv2plug.in/ns/ext/time#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

plug:attack
	a lv2:Parameter ;
	rdfs:label "Attack" ;
	pg:group plug:paramgroup_adsr ;
	rdfs:range atom:Float ;
	lv2:default 0.101 ;
	lv2:minimum 0.001 ;
	lv2:maximum 0.5 .

plug:decay
	a lv2:Parameter ;
	rdfs:label "Decay" ;
	pg:group plug:paramgroup_adsr ;
	rdfs:range atom:Float ;
	lv2:default 0.101 ;
	lv2:minimum 0.001 ;
	lv2:maximum 0.5 .

plug:sustain
	a lv2:Parameter ;
	rdfs:label "Sustain" ;
	pg:group plug:paramgroup_adsr ;
	rdfs:range atom:Float ;
	lv2:default 1 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:release
	a lv2:Parameter ;
	rdfs:label "Release" ;
	pg:group plug:paramgroup_adsr ;
	rdfs:range atom:Float ;
	lv2:default 0.101 ;
	lv2:minimum 0.001 ;
	lv2:maximum 0.5 .

plug:transpose
	a lv2:Parameter ;
	rdfs:label "Transpose" ;
	pg:group plug:paramgroup_additive ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum -24 ;
	lv2:maximum 24 .

plug:tilt
	a lv2:Parameter ;
	rdfs:label "Spectral Tilt" ;
	pg:group plug:paramgroup_additive ;
	rdfs:range atom:Float ;
	lv2:default 0.5 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:spectral%20stretch
	a lv2:Parameter ;
	rdfs:label "Spectral Stretch" ;
	pg:group plug:paramgroup_additive ;
	rdfs:range atom:Float ;
	lv2:default 1 ;
	lv2:minimum 0.125 ;
	lv2:maximum 4 .

plug:frequency%20shift
	a lv2:Parameter ;
	rdfs:label "Spectral Shift" ;
	pg:group plug:paramgroup_additive ;
	rdfs:range atom:Float ;
	lv2:default 0 ;
	lv2:minimum -1000 ;
	lv2:maximum 1000 .

plug:gain
	a lv2:Parameter ;
	rdfs:label "Gain" ;
	pg:group plug:paramgroup_output ;
	rdfs:range atom:Float ;
	lv2:default 0.7 ;
	lv2:minimum 0 ;
	lv2:maximum 1 .

plug:paramgroup_adsr
	a pg:Group ;
	lv2:symbol "paramgroup_adsr" ;
	lv2:name "ADSR" .

plug:paramgroup_additive
	a pg:Group ;
	lv2:symbol "paramgroup_additive" ;
	lv2:name "Additive" .

plug:paramgroup_output
	a pg:Group ;
	lv2:symbol "paramgroup_output" ;
	lv2:name "Output" .

plug:output_group_1
	a pg:OutputGroup ;
	lv2:name "Output" ;
	lv2:symbol "output_group_1" .

<https://nvssynthesis.github.io//plugins/tsara_additive>
	ui:ui <https://nvssynthesis.github.io//plugins/tsara_additive:UI> ;
	a lv2:InstrumentPlugin ;
	doap:name "tsara_additive" ;
	doap:description "tsara_additive" ;
	lv2:minorVersion 0 ;
	lv2:microVersion 0 ;
	doap:maintainer [
		a foaf:Person ;
		foaf:name "nvssynthesis" ;
		foaf:homepage <https://nvssynthesis.github.io/> ;
		foaf:mbox <nicholas93solem@gmail.com> ;
	] ;
	doap:release [
		a doap:Version ;
		doap:revision "1.0.0" ;
	] ;
	lv2:optionalFeature
		lv2:hardRTCapable ;
	lv2:extensionData
		state:interface ;
	lv2:requiredFeature
		urid:map ,
		opts:options ,
		bufs:boundedBlockLength ;
	pg:mainOutput plug:output_group_1 ;
	patch:writable
		plug:attack ,
		plug:decay ,
		plug:sustain ,
		plug:release ,
		plug:transpose ,
		plug:tilt ,
		plug:spectral%20stretch ,
		plug:frequency%20shift ,
		plug:gain ;
	patch:readable
		plug:attack ,
		plug:decay ,
		plug:sustain ,
		plug:release ,
		plug:transpose ,
		plug:tilt ,
		plug:spectral%20stretch ,
		plug:frequency%20shift ,
		plug:gain ;
	lv2:port [
		a lv2:OutputPort , lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "audio_out_1" ;
		lv2:name "Audio Out 1" ;
		pg:group plug:output_group_1 ;
		lv2:designation <http://lv2plug.in/ns/ext/port-groups#center> ;
	] , [
		a lv2:InputPort , atom:AtomPort ;
		rsz:minimumSize 8840 ;
		atom:bufferType atom:Sequence ;
		atom:supports
			midi:MidiEvent ,
			patch:Message ,
			time:Position ;
		lv2:designation lv2:control ;
		lv2:index 1 ;
		lv2:symbol "in" ;
		lv2:name "In" ;
	] , [
		a lv2:OutputPort , atom:AtomPort ;
		rsz:minimumSize 8840 ;
		atom:bufferType atom:Sequence ;
		atom:supports
			patch:Message ;
		lv2:designation lv2:control ;
		lv2:index 2 ;
		lv2:symbol "out" ;
		lv2:name "Out" ;
	] , [
		a lv2:OutputPort , lv2:ControlPort ;
		lv2:designation lv2:latency ;
		lv2:symbol "latency" ;
		lv2:name "Latency" ;
		lv2:index 3 ;
		lv2:portProperty lv2:reportsLatency , lv2:integer , lv2:connectionOptional , pprop:notOnGUI ;
		units:unit units:frame ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:designation lv2:freeWheeling ;
		lv2:symbol "freeWheeling" ;
		lv2:name "Free Wheeling" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:index 4 ;
		lv2:portProperty lv2:toggled , lv2:connectionOptional , pprop:notOnGUI ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:designation lv2:enabled ;
		lv2:symbol "enabled" ;
		lv2:name "Enabled" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:index 5 ;
		lv2:portProperty lv2:toggled , lv2:connectionOptional , pprop:notOnGUI ;
	] ;
	opts:supportedOption
		bufs:maxBlockLength .
